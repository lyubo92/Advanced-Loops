import java.util.Scanner;

public class factorial {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = Integer.parseInt(input.nextLine());
        int a = 1;
        for (int i = n; i > 0; i--) {
            n = a * i;
            a = n;
        }
        System.out.println(a);
    }
}
